                        <picture>
                            <source srcset={{product.image}} type="image/jpg">
                            <img style="max-height: 500px;" src={{product.image}} alt="printscreen-04" class="img-fluid lift">
                        </picture>
                        <h3 class="productNameGrow">{{product.product_name}}</h3>
                        <p class="textCenterGrow">{{product.details}}</p>
                    </div>
                </a>
            {% endfor %}
        </div>
    </br>
    <div class="d-flex justify-content-center widthOneHun">
